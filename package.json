  "directories": {
    "example": "src/examples"
  },
  "files": [
    "dist/index.js"
  ],
  "homepage": "https://unicef.github.io/react-org-chart",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unicef/react-org-chart.git"
  },
  "scripts": {
